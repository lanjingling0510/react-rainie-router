{
  "name": "react-rainie-router",
  "version": "1.0.8",
  "description": "",
  "amdName": "reactRainieRouter",
  "main": "dist/react-rainie-router.js",
  "minified:main": "dist/react-rainie-router.min.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm-run-all clean transpile minify size",
    "transpile": "rollup -c rollup.config.js -m ${npm_package_main}.map -f umd -n $npm_package_amdName -o $npm_package_main",
    "minify": "uglifyjs $npm_package_main -cm -o $npm_package_minified_main -p relative --in-source-map ${npm_package_main}.map --source-map ${npm_package_minified_main}.map",
    "size": "size=$(gzip-size $npm_package_minified_main | pretty-bytes) && echo \"gzip size: $size\"",
    "start": "webpack-dev-server",
    "release": "node_modules/.bin/release",
    "preview-release": "changelog -t preview -s",
    "mocha": "./node_modules/.bin/mocha --compilers js:babel-core/register --require ./test/helper.js --recursive",
    "test": "./node_modules/.bin/nyc npm run mocha",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-es2015-minimal-rollup": "^2.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "gzip-size-cli": "^1.0.0",
    "html-webpack-plugin": "^2.21.0",
    "jsdom": "^9.8.3",
    "mocha": "2.3.3",
    "mocha-lcov-reporter": "^1.2.0",
    "npm-run-all": "^3.0.0",
    "nyc": "^8.1.0",
    "pretty-bytes-cli": "^2.0.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "rf-release": "^0.4.0",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-json": "^2.0.1",
    "rollup-plugin-memory": "^2.0.0",
    "rollup-plugin-node-globals": "^1.0.6",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}